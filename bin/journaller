#!/bin/bash

shopt -s extglob globstar nullglob

for file in $(dirname $0)/../lib/**/!(_*); do
  [[ -f $file ]] && . $file
done

load_config
echo $JOURNAL_DIR

command="cmd_${1/-/_}"
if commands_exist $command; then
  $command "${@:2}"
else
  if [[ -n "$1" ]]; then
    echo "'$1' is not a journaller command."
  fi
  cmd_help
fi
