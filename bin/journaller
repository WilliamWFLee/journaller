#!/bin/bash

for file in $(dirname $BASH_SOURCE)/../lib/commands/*; do
  . $file
done

. ~/.journallerrc
. $(dirname $BASH_SOURCE)/../lib/command_test

command="_journaller_$1"
if _commands_exists $command; then
  $command "${@:2}"
else
  if [[ -n "$1" ]]; then
    echo "'$1' is not a journaller command."
  fi
  _journaller_help
fi
