#!/bin/bash

for file in $(dirname $BASH_SOURCE)/../lib/commands/*; do
  . $file
done

. ~/.journallerrc
command="_journaller_$1"
type $command > /dev/null 2>&1
if [[ $? -ne 0 ]]; then
  if [[ -n "$1" ]]; then
    echo "'$1' is not a journaller command."
  fi
  _journaller_help
else
  $command "${@:2}"
fi
